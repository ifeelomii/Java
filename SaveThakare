//Library Class

import java.util.HashMap;
import java.util.Map;

public class Library{
    private int number_of_books;
    private String name;
    private Map<String, Integer> hm = new HashMap<>();
    public int getNumber_of_books() {
        return number_of_books;
    }
    public void setNumber_of_books(int number_of_books) {
        this.number_of_books = number_of_books;
    }
    public String getName() {
        return name;
    }
    public void setName(String name) {
        this.name = name;
    }
    public Map<String, Integer> getHm() {
        return hm;
    }
    public void setHm(Map<String, Integer> hm) {
        this.hm = hm;
    }

    
}
